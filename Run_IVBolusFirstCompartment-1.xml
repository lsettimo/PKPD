<Lems>
    
    <Target component="sim1"/>
    
    <Include file="Simulation.xml"/>
    
    <Include file="IVBolusFirstCompartment-1.xml"/>
    
    <IVBolusFirstCompartment id="ivbolus" halflife="3" ab0="3"/>
    
    
    <Simulation id="sim1" length="20s" step="0.01s" target="ivbolus" method="rk4">
        
        <Display id="d1" title="ab dose timeseries"
            timeScale="1s" xmin="0" xmax="20" ymin="0" ymax="5">
            <Line id ="x" quantity="ab" scale="1"
            color="#000000" timeScale="1s"/>
        </Display>
        
    </Simulation>
    
</Lems>
